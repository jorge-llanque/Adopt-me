olumn":2040}},{"source":"SearchParams.js","name":"animals","original":{"line":97,"column":36},"generated":{"line":1,"column":2048}},{"source":"SearchParams.js","name":"SearchParams","original":{"line":102,"column":15},"generated":{"line":1,"column":2056}},{"source":"SearchParams.js","name":"SearchParams","original":{"line":102,"column":15},"generated":{"line":1,"column":2060}},{"source":"SearchParams.js","name":"SearchParams","original":{"line":102,"column":15},"generated":{"line":1,"column":2062}},{"source":"SearchParams.js","name":"SearchParams","original":{"line":102,"column":15},"generated":{"line":1,"column":2064}},{"source":"SearchParams.js","name":"SearchParams","original":{"line":102,"column":15},"generated":{"line":1,"column":2072}},{"source":"SearchParams.js","name":"SearchParams","original":{"line":102,"column":15},"generated":{"line":1,"column":2080}}],"sources":{"SearchParams.js":"import { useState, useEffect, useContext } from 'react'\r\nimport ThemeContext from './ThemeContext'\r\nimport useBreedList from './useBreedList'\r\nimport Results from './Results'\r\nimport getAnimals from './services/getAnimals'\r\n\r\nconst ANIMALS = ['bird', 'cat', 'dog', 'rabbit', 'reptile']\r\n\r\nconst SearchParams = () => {\r\n  const [animal, updateAnimal] = useState('')\r\n  const [location, updateLocation] = useState('')\r\n  const [breed, updateBreed] = useState('')\r\n  const [pets, setPets] = useState([])\r\n  const [animals, setAnimals] = useState([])\r\n  const [breeds] = useBreedList(animal)\r\n  const [theme, setTheme] = useContext(ThemeContext)\r\n\r\n  useEffect(() => {\r\n    getAnimals(setAnimals)\r\n    requestPets()\r\n  }, []) // eslint-disable-line react-hooks/exhaustive-deps\r\n\r\n  async function requestPets() {\r\n    const res = await fetch(\r\n      `https://pets-v2.dev-apis.com/pets?animal=${animal}&location=${location}&breed=${breed}`\r\n    )\r\n    const json = await res.json()\r\n\r\n    setPets(json.pets)\r\n  }\r\n\r\n  return (\r\n    <div className='search-params'>\r\n      <form\r\n        onSubmit={e => {\r\n          e.preventDefault()\r\n          requestPets()\r\n        }}\r\n      >\r\n        <label htmlFor='location'>\r\n          Location\r\n          <input\r\n            id='location'\r\n            value={location}\r\n            placeholder='Location'\r\n            onChange={e => updateLocation(e.target.value)}\r\n          />\r\n        </label>\r\n        <label htmlFor='animal'>\r\n          Animal\r\n          <select\r\n            id='animal'\r\n            value={animal}\r\n            onChange={e => updateAnimal(e.target.value)}\r\n            onBlur={e => updateAnimal(e.target.value)}\r\n          >\r\n            <option />\r\n            {ANIMALS.map(animal => (\r\n              <option key={animal} value={animal}>\r\n                {animal}\r\n              </option>\r\n            ))}\r\n          </select>\r\n        </label>\r\n        <label htmlFor='breed'>\r\n          Breed\r\n          <select\r\n            disabled={!breeds.length}\r\n            id='breed'\r\n            value={breed}\r\n            onChange={e => updateBreed(e.target.value)}\r\n            onBlur={e => updateBreed(e.target.value)}\r\n          >\r\n            <option />\r\n            {breeds.map(breed => (\r\n              <option key={breed} value={breed}>\r\n                {breed}\r\n              </option>\r\n            ))}\r\n          </select>\r\n        </label>\r\n        <label htmlFor='theme'>\r\n          Theme\r\n          <select\r\n            value={theme}\r\n            onChange={e => setTheme(e.target.value)}\r\n            onBlur={e => setTheme(e.target.value)}\r\n          >\r\n            <option value='peru'>Peru</option>\r\n            <option value='darkblue'>Dark Blue</option>\r\n            <option value='chartreuse'>Chartreuse</option>\r\n            <option value='mediumorchid'>Medium Orchid</option>\r\n          </select>\r\n        </label>\r\n        <button style={{ backgroundColor: theme }}>Submit</button>\r\n      </form>\r\n      <Results pets={pets} animals={animals} />\r\n    </div>\r\n  )\r\n}\r\n\r\nexport default SearchParams\r\n"},"lineCount":null}},"error":null,"hash":"9bc293a5b8e82bcf0c16d119dbc4a051","cacheData":{"env":{}}}